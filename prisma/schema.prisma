generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @default(uuid())
  name       String
  email      String   @unique
  password   String   @default("default_password")
  created_at DateTime @default(now())
  tasks      Create[]
}

model Create {
  id            String   @id @default(uuid())
  title         String
  description   String
  category      String
  applyDate     DateTime
  interviewDate DateTime
  status        String
  companyName   String
  location      String
  jobId         String
  created_at    DateTime @default(now())
  user          User     @relation(fields: [userId], references: [id])
  userId        String
}
